<form action="/tienda/editarproducto/{{tienda.id}}" method="POST">
    <input type="text" name="nombre" id="nombre" value="{{tienda.nombre}}"><br>
    <input type="text" name="precio" id="precio" value="{{tienda.precio}}"><br>
    <input type="text" name="categoria" id="categoria" value="{{tienda.categoria}}"><br>
    <input type="text" name="stock" id="stock" value="{{tienda.stock}}"><br>
    <button id="btn-subir-producto">Subir</button>
</form>